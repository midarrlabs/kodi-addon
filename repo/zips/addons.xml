            <info compressed="false">https://raw.githubusercontent.com/midarrlabs/repository.midarr/main/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/midarrlabs/repository.midarr/main/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/midarrlabs/repository.midarr/main/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Official Midarr for Kodi repository</summary>
        <description lang="en">Official Midarr for Kodi repository</description>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
        </assets>
        <nofanart>true</nofanart>
